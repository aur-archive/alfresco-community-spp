post_install() {
  echo "======> ATTENTION <======"
  echo "You need to install this module into the web application"
  echo "with apply_amps.sh when the servelet container is down."
  echo "You will also need to remove any expanded directories"
  echo "(such as /srv/tomcat/webapps/alfresco)."
}

